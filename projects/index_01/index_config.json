{
  "host_name": "DevOps",
  "port": "9200",
  "index_group": "logs_group",
  "index_name": "application_logs",
  "index_pattern": "application-logs-*",
  "timestamp_field": "@timestamp",
  "x_time_back": "1d",
  "granularity": "1h",
  "file_size_mb": 250,
  "elasticdump_limit":10000,
  "elasticdump_timeout":21600,
  "timezone": "America/Los_Angeles",

  "avg_records_per_query_window": 100000,
  "base_source_to_stage_minutes": 10,    
  "base_stage_to_target_minutes": 5, 
  "stage_to_target_power_factor": 0.6,
  
  "base_audit_minutes": 2,               
  "skew_factor": 0.5,
  "max_retry_attempts": 3,
  "retry_delay_seconds": 30,
  "must_exist": ["user_id", "action"],
  "exclude_null": ["timestamp"],
  "custom_query": "",
  "bucket_name": "{env}-data-bucket",
  "s3_prefix": "logs/{env}/{index_group}/{index_name}/",

  "file_prefix": "{index_name}_{timestamp}",
  "region": "us-west-1",
  "elasticdump_options": "--size=10000 --limit=250",
  "target_database": "{env}_{index_group}_{index_name}_db",
  "target_schema": "{env}_{index_name}_sch",
  "target_table": "{env}_{index_group}_{index_name}_tbl",
  "drive_database": "{env}_xas",
  "drive_schema": "abs", 
  "drive_table": "x1",
  "snowpipe_name": "{index_name}_pipe",
  "enable_crazy_retry_logic": true,
  "warehouse": "COMPUTE_WH",
  "role": "DATA_LOADER_ROLE",
  "pause_duration_seconds": 60,
  
  "_comment": "=== DRIVE TABLE OVERRIDES ===",
  "source_name": "custom_elasticsearch",
  "target_name": "custom_snowflake", 
  "stage_name": "custom_s3",
  "pipeline_name": "CUSTOM_ES_TO_S3_TO_SF",
  "pipeline_priority": 2.5,
  "triggered_by": "CUSTOM_AIRFLOW",
  "source_category": "custom_{index_group}",
  "target_category": "custom_{env}_db.custom_schema.{index_name}_table",
  "source_discovery_expected_duration_minutes": 5,
  "source_to_stage_transfer_expected_duration_minutes": 15,
  "stage_to_target_transfer_expected_duration_minutes": 12,
  "audit_expected_duration_minutes": 3,
  "accepted_tolerance_percentage": 0.5,
  "source_discovery_is_enabled": false,
  "audit_is_enabled": true,
  "max_pending_records":3
}